{
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": ".",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "node",
    "isolatedModules": false,
    "types": ["cypress"],
    "typeRoots": ["./support/index.d.ts"]
  },
  "include": ["./**/*.ts"]
}
